{
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "extensions": [
          ".ios.js",
          ".android.js",
          ".js",
          ".jsx",
          ".ts",
          ".tsx",
          ".json",
        ],
        "alias": {
          "*": ["src/*"],
        },
      },
    ],
  ],
}
